{
  "private": false,
  "name": "@icare-jp/vue-props-story-fn-factory",
  "version": "0.0.3",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "eslint --ext .tsx,.ts",
    "lintfix": "prettier --write '**/*.{ts,tsx}' && eslint --ext .tsx,.ts --fix .",
    "publish": "npm publish --access public"
  },
  "description": "vue-props-story-fn-factory",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icare-jp-oss/vue-props-story-fn-factory.git"
  },
  "keywords": [],
  "author": "kahirokunn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icare-jp-oss/vue-props-story-fn-factory/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@storybook/addons": "^6.0.5",
    "@types/node": "^14.0.27",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "storybook": "^6.0.5",
    "typescript": "^3.9.7",
    "vue": "^2.6.11"
  }
}
